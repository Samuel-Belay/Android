{
    "name": "Android Emulator Codespace",
    "build": {
        "dockerfile": "Dockerfile",
        "context": "."
    },
    "forwardPorts": [5901],
    "remoteUser": "root",
    "postCreateCommand": "/usr/local/bin/start-android.sh",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.cpptools",
                "vscjava.vscode-java-pack"
            ]
        }
    }
}
