{
  "name": "Android Devcontainer",
  "dockerFile": "Dockerfile",
  "context": ".",
  "mounts": [
    "source=${localWorkspaceFolder}/.android-sdk,target=/opt/android-sdk,type=bind,consistency=cached"
  ],
  "remoteUser": "vscode",
  "forwardPorts": [5901],
  "postStartCommand": "/usr/local/bin/start-android.sh"
}
