{
  "name": "Android Emulator Codespace",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "forwardPorts": [5901],
  "postCreateCommand": "bash /workspaces/$(basename $PWD)/.devcontainer/setup-android.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.cpptools",
        "ms-python.python"
      ]
    }
  },
  "remoteUser": "vscode"
}
