{
    "name": "Android Pixel 6 Codespace",
    "build": {
        "dockerfile": "Dockerfile",
        "context": "."
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "vscode.android-emulator",
                "ms-vscode.cpptools",
                "ms-vscode.vscode-java-pack"
            ]
        }
    },
    "remoteUser": "vscode",
    "postStartCommand": "/usr/local/bin/start-android.sh",
    "forwardPorts": [5901]
}
